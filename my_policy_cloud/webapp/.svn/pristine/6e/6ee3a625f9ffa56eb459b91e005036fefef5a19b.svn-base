<script>
      //jquery验证手机号码
      function show() {  // 注册的页的正则验证
      if ($("#email").val() == "")
      {
      alert("账号不能为空！");
      
      $("#email").focus();
      return false;
      }
      if (!$("#email").val().match(/^(((13[0-9]{1})|159|153)+\d{8})$/) &&!$("#email").val().match(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/))
      {
      alert("手机邮箱格式不正确！");
      $("#email").focus();
      return false;
      }
      if($('#mobile').val()=="" )
      {
      alert("密码不能为空！");
      }
      if(!$('#checked').is(':checked')) {
      alert('请选择同意')
      }
      return true;
      
      
      }
      </script>
