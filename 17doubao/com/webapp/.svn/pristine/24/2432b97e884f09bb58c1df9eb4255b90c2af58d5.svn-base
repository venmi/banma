$(function () {
    $("#saveBtn").click(function () {
        var ajaxOptions = {
            url: "/shareperson",
            type: 'post',
            dataType: 'json',
            timeout: 100000,
            cache: false,
            success: function (responseJson) {
                if (responseJson.success) {
                    location.href = "/myshareperson";
                } else {
                    alert(responseJson.msg)
                }
            }
        };
        $("#savePersonForm").ajaxSubmit(ajaxOptions);
    });

    $("#cancelBtn").click(function () {
        location.href = "/myshareperson"
    })
});
