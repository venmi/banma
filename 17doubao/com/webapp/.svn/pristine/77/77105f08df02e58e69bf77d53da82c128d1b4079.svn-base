$base_color: #ffffff;
$border_color: #ddd;
$background_color: #f0f0f0;
$font_family: "Helvetica Neue", Helvetica, "Hiragino Sans GB", "STHeitiSC-Light", "Microsoft YaHei", "微软雅黑", Arial, sans-serif;
$font_family_num: arail;
$orange_color: #EB5508;
$gray_color: #666;
$dark_gray_color: #333;
$white_color: #fff;
$black: #333;
$red: #FF1A1A;
$blue: #3786EC;
$border_radius: 4px;
$base_height: 34px;
$colum: 15px;
$margin_top: 20px;
$font_size: 12px;
$margin_top: 20px;
$title_width: 100%;
$font_size: 12px;
$text_color: #999999;
$box_shadow_color: rgba(0, 0, 0, .1);

@font-face {
  font-family: "iconfont";
  src: url('../fonts/iconfont.eot'); /* IE9*/
  src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */ url('../fonts/iconfont.woff') format('woff'), /* chrome、firefox */ url('../fonts/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/ url('../fonts/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

html {
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

body {
  font-family: $font_family;
  background-color: $background_color;
  color: $dark_gray_color;
}

a {
  color: $orange_color;

  &:hover {
    text-decoration: none;
    color: $orange_color;
  }
  &:focus {
    text-decoration: none;
    color: $orange_color;
  }
}

ul, li {
  list-style: none;
  margin: 0;
  padding: 0;
}

textarea {
  resize: none;
}

//dctxf
.float_side {
  display: none;
  box-shadow: 0 0 10px $box_shadow_color;
  -moz-box-shadow: 0 0 10px $box_shadow_color;
  -o-box-shadow: 0 0 10px $box_shadow_color;
  -webkit-box-shadow: 0 0 10px $box_shadow_color;

  @media (min-width: 1000px) {
    display: block;
  }
  width: $colum * 4;
  background-color: $white_color;
  position: fixed;
  z-index: 99999;
  right: $colum;
  top: 20%;

  ul {
    position: relative;

    li {
      a {
        text-align: center;
        display: block;
        width: $colum * 4;
        height: $colum * 4;
        color: $gray_color;
        font-size: $font_size;
        border-bottom: 1px solid $border_color;

        i {
          color: $orange_color;
          font-size: $font_size * 2;
        }
        &:hover {
          background-color: $orange_color;
          border-bottom-color: $orange_color;
          color: $white_color;
          transition: background-color .5s, color .5s, border-bottom-color .5s;
          -moz-transition: background-color .5s, color .5s, border-bottom-color .5s;
          -o-transition: background-color .5s, color .5s, border-bottom-color .5s;
          -webkit-transition: background-color .5s, color .5s, border-bottom-color .5s;

          i {
            color: $white_color;
            transition: color .5s;
            -moz-transition: color .5s;
            -o-transition: color .5s;
            -webkit-transition: color .5s;
          }
        }
        &:last-child {
          a {
            border-bottom: none;
          }
        }
      }
    }
  }
  .wechat_qrcode {
    display: none;
    position: absolute;
    top: 0;
    right: $colum * 4;
    width: $colum * 12;
    height: $colum * 12;

    img {
      width: 100%;
    }
  }
}

.top {
  font-size: $font_size;
  border-bottom: 1px solid $border_color;
  height: 40px;
  line-height: 40px;
  color: #646464;
  background-color: $white_color;

  a {
    color: $dark_gray_color;

    &:hover {
      text-decoration: none;
      color: $orange_color;
    }
  }
  .back_to_home {
    i {
      color: $gray_color;
    }
  }
  .fast_menu {
    a {
      color: $dark_gray_color;

      em {
        font-style: normal;
      }
    }
    .login_register, .integral_policy {
      background-image: url(../../resources/images/line1.png);
      background-repeat: no-repeat;
      background-position: center right;
      padding-right: 10px;

      a {
        color: $orange_color;

        &:hover, &:focus {
          color: $orange_color - 60;
        }
      }
      i {
        color: $orange_color;
      }
      b {
        font-family: $font_family_num;
        font-weight: bold;
        padding: 0 5px;
        color: $orange-color;
      }
    }
    .login_register {
      .login {
        float: left;
        background-image: url(../../resources/images/line1.png);
        background-repeat: no-repeat;
        background-position: center right;
        padding-right: 10px;
      }
      .register {
        float: left;
        padding-left: 10px;
      }
      span {
        margin-left: $colum - 10;

        em {
          @media (max-width: 375px) {
            display: none;
          }
        }
        a {
          color: $gray_color;

          i {
            color: $gray_color;
          }
        }
      }
    }
    .integral_policy, .messages {
      padding-left: 10px;
    }
    .messages {
      span {
        background-color: $orange_color;
        color: $white_color;
        border-radius: 40px;
        padding: 0 10px;
        margin-left: 5px;
      }
    }
  }
}

.navbar {
  border: none;
  background-color: $white_color;
  min-height: 80px;
  margin-bottom: 0;
  .container {

    @media screen and (min-width: 768px) {
      overflow: hidden;
    }
    @media screen and (max-width: 768px) {
      width: 100%;
      overflow: hidden;
    }
  }
  .navbar-header {

    .logo {
      height: 80px;
      margin-left: 10px;
      margin-right: 20px;

      .pic {
        float: left;

        img {
          width: 129px;
          height: 52px;
        }
      }
      .text {
        font-size: $font_size * 3;
        font-weight: bold;
        color: $orange_color;
      }
      .slogan {
        display: none;

        @media (min-width: 1000px) {
          display: block;
        }
        float: left;
        line-height: 50px;
        border-left: 1px solid $border_color;
        margin-left: 20px;
        padding-left: 20px;
        font-size: $font_size;
        color: $gray_color;
      }
      padding-top: 10px;
    }
    .navbar-toggle {
      margin-top: 22px;
    }
  }
  .navbar-nav {
    li {
      a {
        line-height: 47px;
        padding: 15px 15px;
        background-color: $white_color;
        color: $dark_gray_color;
        border-bottom: 3px solid $white_color;

        &:focus {
        }
      }
    }
    .active {
      a {
        color: $orange_color;
        border-bottom: 3px solid $orange_color;
        line-height: 47px;
        background-color: $white_color;

        &:hover {
          color: $orange_color;
          background-color: $white_color;
        }
        &:focus {
          background-color: $white_color;
        }
      }
    }
  }
}

.banner {
  position: relative;
  width: 100%;
  overflow: auto;

  .carousel-indicators {
    li {
      *float: left;
    }
  }
  .slogan {
    font-size: $font_size * 1.5;
    padding-bottom: 10%;

    @media (min-width: 768px) {
      font-size: $font_size * 4;
      padding-bottom: 20%;
    }
    text-align: center;
    color: $white_color;
    text-shadow: none;
  }
}

.fast_track {
  margin-top: $colum;

  a {
    display: block;
    color: $orange_color;
    background-color: $white_color;
    text-align: center;
    padding: 10px 0 30px;

    i {
      font-size: $font_size * 6;
    }
    &:hover {
      color: $white_color;
      background-color: $orange_color;
      box-shadow: 0 0 10px $box_shadow_color;
      -moz-box-shadow: 0 0 10px $box_shadow_color;
      -o-box-shadow: 0 0 10px $box_shadow_color;
      -webkit-box-shadow: 0 0 10px $box_shadow_color;
      transition: background-color 1s, color 1s;
      -moz-transition: background-color 1s, color 1s;
      -o-transition: background-color 1s, color 1s;
      -webkit-transition: background-color 1s, color 1s;
    }
  }
}

.product {
  margin-bottom: 20px;

  h4 {
    border-bottom: 1px solid $border_color;
    padding-bottom: 20px;
    padding-top: 10px;
    margin-bottom: 20px;

    a {
      color: $black;
    }
  }
  img {
    width: 100%;
  }
  .name {
    background-color: $white_color;
    height: $colum * 3;
    line-height: $colum * 3;
    overflow: hidden;
    font-size: $font_size * 1.5;
    font-weight: bold;
    padding: 0 $colum;
  }
  .info {
    padding: 0 $colum;
    background-color: $white_color;
    height: $colum * 4;
    line-height: $colum * 1.4;
    overflow: hidden;
    font-size: $font_size;
    color: $gray_color;
  }
  .price {
    background-color: $white_color;
    text-align: center;
    line-height: $colum * 2;
    font-family: $font_family_num;
    font-size: $colum * 2;
    padding-top: $colum;
    color: $orange_color;

    span {
      font-size: $font_size;
      line-height: $font_size;
    }
  }
  .button {
    background-color: $white_color;
    padding: $colum;
    margin-bottom: $colum;

    a {
      display: block;
      background-color: $orange_color;
      border-radius: $border_radius;
      text-align: center;
      line-height: $base_height;
      color: $white_color;

      &:hover {
        background-color: $orange_color - 20;
      }
    }
  }
}

.activity {
  background-color: $white_color;

  h4 {
    border-bottom: 1px solid $border_color;
    padding-bottom: 20px;
    padding-top: 10px;
    margin-bottom: 20px;

    a {
      color: $black;
    }
  }
  img {
    width: 100%;
  }
}

.news, .news_list {
  background-color: $white_color;

  h4 {
    border-bottom: 1px solid $border_color;
    padding-bottom: 20px;
    padding-top: 10px;
    margin-bottom: 0;

    a {
      color: $black;

      span {
        color: $gray_color;
        float: right;
        font-family: $font_family_num;
        font-size: $font_size * 2;

        &:hover {
          color: $orange_color;
        }
      }
    }
  }
  ul.text_list {
    li {
      height: $colum * 2.6;
      line-height: $colum * 2.6;
      font-size: $colum;
      border-bottom: 1px solid $border_color;

      a {
        color: $dark_gray_color;

        &:hover {
          color: $black;
        }
      }
      .hot {
        color: $red;
      }
    }
  }
  ul.pic_list {
    li {
      height: $colum * 6;
      line-height: $colum;
      overflow: hidden;

      .pic {
        width: $colum * 8;
        height: $colum * 6;
        margin: 10px 10px 0 0;
        float: left;

        img {
          width: 100%;
          height: 100%;
        }
      }
      h5 {
        height: $colum + 2;
        overflow: hidden;
        font-size: $colum;

        a {
          color: $dark_gray_color;

          &:hover {
            color: $black;
          }
        }
      }
      .content {
        height: $colum * 4;
        line-height: $colum * 1.2;
        overflow: hidden;
        font-size: $colum * 0.8;

        a {
          color: $gray_color;
        }
      }
    }
  }
}

.news_list {
  margin-top: $colum;
  padding: $colum;

  li {
    .time {
      @media (max-width: 768px) {
        display: none;
      }
      font-size: $font_size;
      color: $gray_color;
      float: right;
    }
  }
}

.firend_links {
  background-color: $white_color;

  h4 {
    border-bottom: 1px solid $border_color;
    padding-bottom: 20px;
    padding-top: 10px;
  }
  img {
    width: 60%;
    margin: 10%;
  }
}

.footer {
  margin-top: $colum;
  padding-bottom: 20px;
  padding-top: 20px;
  background-color: $white_color;

  .menu {
    display: none;

    @media (min-width: 768px) {
      display: block;
    }
    ul {
      line-height: 40px;
      font-size: $font_size * 1.2;
      color: $black;

      li {
        line-height: 24px;

        a {
          color: $dark_gray_color;
          font-size: $font_size;
        }
      }
    }
  }
  .logo {
    display: none;

    @media (min-width: 768px) {
      display: block;
    }
    padding-top: 30px;

    img {
      width: 140px;
      height: 74px;
    }
  }
  .qrcode {
    display: none;

    @media (min-width: 768px) {
      display: block;
    }
    p {
      font-size: $font_size * 1.2;
      color: $orange_color;
      padding-right: 20px;
    }
    img {
      width: 140px;
      height: 140px;
    }
  }
  .copyright {
    .copy {
      @media (min-width: 768px) {
        float: left;
      }
    }
    .icp {
      float: left;

      @media (min-width: 768px) {
        float: right;
      }
    }
    border-top: 1px solid $border_color;
    margin-top: $colum;
    padding-top: $colum;
    font-size: $font_size;
  }
}

.my_activity {
  margin-top: $colum;
  background-color: $white_color;
  overflow: hidden;

  .tab_nav {
    background-color: $white_color;
    height: $colum *  2.8;
    line-height: $colum * 2.8;
    overflow: hidden;
    padding: 0;
    border-bottom: 1px solid $border_color;

    li {
      float: left;
      width: $colum * 6;
      text-align: center;
      border-top: 3px solid $white_color;

      a {
        color: $dark_gray_color;
      }
      &.active {
        border-top: 3px solid $orange_color;

        a {
          color: $orange_color;
        }
      }
      &:hover {
        a {
          color: $orange_color;
        }
      }
    }
  }
  .list {
    li {
      overflow: hidden;
      border-bottom: 1px solid $border_color;

      &:last-child {
        border-bottom: none;
      }
      .pic {
        margin: $colum 0;

        img {
          width: 100%;
        }
      }
      .content {
        margin: $colum 0;

        .name {
          font-size: $font_size * 1.4;
          line-height: $font_size * 1.4;
          font-weight: bold;

          a {
            color: $black;

            &:hover {
              color: $orange_color;
            }
          }
        }
        .info {
          margin: $colum 0;
          font-size: $font_size * 1.2;
          line-height: $font_size * 2;
        }
        .button {
          a {
            display: block;
            border-radius: $border_radius;
            height: $base_height;
            line-height: $base_height;
            text-align: center;
            color: $white_color;
          }
        }
        .able {
          a {
            background-color: $orange_color;

            &:hover {
              background-color: $orange_color - 20;
            }
          }
        }
        .disable {
          a {
            background-color: $gray_color;
            cursor: not-allowed;
          }
        }
      }
    }
  }
}

.pagination_box {
  background-color: $white_color;
  padding: 0 $colum;

  .pagination, .pagination-lg {
    li {
      a {
        color: $orange_color;
      }
    }
    .active {
      a {
        color: $white_color;
        border-color: $orange_color;
        background-color: $orange_color;
      }
    }
  }
}

.user_center, .hlep_center {
  margin-top: $colum;

  .side {
    background-color: $white_color;

    .pic {
      position: relative;

      .edit_photo {
        display: none;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0, 0, 0, .5);

        i {
          position: absolute;
          top: 50%;
          left: 50%;
          line-height: 100%;
          margin-left: - $font_size * 3;
          margin-top: - $font_size * 3;
          color: $white_color;
          font-size: $font_size * 6;
        }
      }
      img {
        width: 100%;
      }
    }
    .name {
      text-align: center;
      border-bottom: 1px solid $border_color;
      padding: $colum 0;
      font-size: $font_size *1.2;
    }
    .integral_policynum {
      overflow: hidden;

      .integral, .policynum {
        text-align: center;
        border-bottom: 1px solid $border_color;
        width: 50%;
        float: left;
        line-height: $font_size * 1.6;
        padding: ($colum - 5) 0;

        a {
          font-family: $font_family_num;
          color: $orange_color;
          font-size: $font_size * 1.4;

          span {
            font-size: $font_size;
            color: $dark_gray_color;
          }
        }
      }
      .integral {
        border-right: 1px solid $border_color;
      }
    }
    .menu {
      padding: $colum 0;

      li {
        a {
          display: block;
          text-align: center;
          font-size: $font_size * 1.2;
          line-height: $font_size * 3;
          color: $dark_gray_color;
          border-right: 3px solid $white_color;

          i {
            color: $orange_color;
            font-size: $font_size * 2;
            line-height: $font_size * 3;
            padding-right: $colum - 10;
          }
        }
        &:hover, &.acitve {
          a {
            border-right: 3px solid $orange_color;
          }
        }
      }
    }
    @media (max-width: 1000px) {
      margin-bottom: $colum;

      .pic {
        text-align: center;
        padding: $colum;

        .edit_photo {
          position: absolute;
          top: 50%;
          left: 50%;
          width: $colum * 8;
          height: $colum * 8;
          border-radius: 50%;
          margin-left: - $colum * 4;
          margin-top: - $colum * 4;
          background-color: rgba(0, 0, 0, .5);

          i {
            position: absolute;
            top: 50%;
            left: 50%;
            line-height: 100%;
            margin-left: - $font_size * 3;
            margin-top: - $font_size * 3;
            color: $white_color;
            font-size: $font_size * 6;
          }
        }
        img {
          width: $colum * 8;
          height: $colum * 8;
          border-radius: 50%;
          text-align: center;
        }
      }
      .name {
        padding-top: 0;
      }
      .menu {
        overflow: hidden;
        padding: 0;

        li {
          float: left;
          width: 25%;
          text-align: center;

          a {
            display: block;
            border-left: none;

            i {
              display: block;
              height: $font_size * 3;
              line-height: $font_size * 3;
            }
            span {
              display: block;
            }
          }
          &:hover, &.acitve {
            a {
              border-right: none;
              background-color: $orange_color;
              color: $white_color;

              i {
                color: $white_color;
              }
            }
          }
        }
      }
    }
  }
  .content {
    background-color: $white_color;
    padding: 0 $colum $colum $colum;

    .title {
      font-size: $font_size * 1.2;
      font-weight: bold;
      padding: $colum 0;
      margin-bottom: $colum;
      border-bottom: 1px solid $border_color;
      color: $dark_gray_color;
    }
  }
  .user_home {
    p {
      line-height: $font_size * 2;

      span {
        display: inline-block;
        width: $colum * 6;
        text-align: right;
        padding-right: $colum - 5;
        font-weight: bold;
      }
      .edit_a {
        margin-left: $colum;
      }
    }
  }
  .user_messages, .modify_date {
    ul.tab_bar {
      overflow: hidden;
      border-bottom: 1px solid $border_color;

      li {
        float: left;
        padding: ($colum - 3) 0 $colum 0;
        border-top: 3px solid $white_color;

        a {
          padding: 0 $colum;
          color: $dark_gray_color;
        }
        &.active {
          border-top: 3px solid $orange_color;

          padding-top: $colum a {
          color: $orange_color;
        }
        }
      }
      .all_read {
        float: right;
        padding: $colum 0 $colum 0;
        color: $dark_gray_color;

        &:hover {
          color: $orange_color;
        }
      }
    }
  }
  .modify_date {
    .basic_data {
      margin-top: $colum;

      .name, .gender, .birth_time, .address, .personality_signature {
        margin-top: $colum;

        .tag {
          line-height: $font_size * 2;
          color: $dark_gray_color;
        }
      }
      .prompt {
        .prompt_span {
          font-size: $font_size;
          color: $gray_color;
        }
        .format {
          display: none;
          font-size: $font_size;
          color: $red;
        }
      }
      .gender {
        label {
          margin-right: $colum;
          margin-bottom: 0;
          color: $dark_gray_color;
          font-weight: normal;
        }
      }
      .textarea {
        width: 100%;
        min-height: $font_size * 6;
        padding: $colum - 10;
        border-radius: $border_radius;
        resize: none;
        outline: none;
      }
      .button {
        background-color: $white_color;
        margin-top: $colum;

        a {
          display: block;
          background-color: $orange_color;
          border-radius: $border_radius;
          text-align: center;
          line-height: $base_height;
          color: $white_color;

          &:hover {
            background-color: $orange_color - 20;
          }
        }
      }
    }
  }
