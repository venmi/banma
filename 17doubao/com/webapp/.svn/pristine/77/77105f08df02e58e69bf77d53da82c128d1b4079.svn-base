.upload_policy, .validation_success {
  background: $base_color;
  padding-bottom: $colum * 2;

  .title {
    padding-left: $colum;
    border-bottom: 1px solid $border_color;
    margin-bottom: $colum;

    h2 {
      color: $orange_color;
      padding-top: $colum;
      font-size: $font_size * 2;
    }
  }
  .share_man{
    padding-left:$colum * 2;

    ul{
      overflow: hidden;
      li{
        font-size: $font_size;
        float: left;
        border: 1px solid $border_color;
        padding: $colum - 10;
        margin-right: $colum - 10;
        margin-bottom: $colum - 10;
        color: $gray_color;
        &:hover{
          background-color: $orange_color;
          color: $white_color;
          border-color: $orange_color;
        }
      }
    }
    .button{
      a{
        display: block;
        width: 100%;
        text-align: center;
        color: $white_color;
        font-size: $font_size * 1.2;
        line-height: $font_size * 1.2;
        padding: ($colum - 5) 0;
        background-color: $orange_color;
        border-radius: $border_radius;
        &:hover{
          background-color: $orange_color - 20;
        }
      }
    }
  }
  .content {
    padding:$colum $colum 0 $colum;

    .state {
      width: 100%;
      margin-bottom: $colum * 2;

      .tag {
        font-size: $font-size +2;
        margin-bottom: $colum * 1;
        padding-bottom: $colum - 10;
        border-bottom: 1px solid $border_color;
      }
      label {
        margin-right: $colum * 1;
        cursor: pointer;

        input {
          margin-right: $colum /2;
          vertical-align: middle;
          height: 15px;
          width: 15px;
        }
        span {
          font-weight: normal;
        }
      }
    }
    .upload_file {
      width: 100%;
      position: relative;
      margin-bottom: $colum * 1;
      overflow: hidden;

      .tag {
        font-size: $font-size +2;
        margin-bottom: $colum * 1;
        padding-bottom: $colum - 10;
        border-bottom: 1px solid $border_color;
      }
      .file_img_cen {
        width: 100%;
        margin-top: $colum;

        .file_img {
          max-width: 150px;
          height: 200px;
          text-align: center;
          margin-right: $colum - 5;
          margin-bottom: $colum - 5;
          position: relative;
          float: left;

          .delete {
            text-align: center;
            font-size: $font_size;
            padding: $colum - 10;
            position: absolute;
            bottom: 0;
            left: 0;
            background-color: $red;
            width: 100%;
            opacity: .8;
            filter: alpha(opacity=.8);

            a {
              display: block;
              color: $white_color;
            }
          }
          img {
            max-height: 100%;
            max-width: 100%;
          }
        }
      }
    }
    .preservation {
      a {
        display: block;
        text-align: center;
        margin-right: 10px;
        background-color: $orange_color;
        line-height: $base_height;
        height: $base_height;
        color: $white_color;
        border-radius: $border_radius;

        &:hover {
          background-color: $orange_color - 20;
        }
        &.btn_gray {
          background-color: $dark_gray_color;
        }
      }
    }
  }
  .success, .failure, .wait {
    text-align: center;
    font-size: $font-size*2;

    .iconfont {
      font-size: $font-size*6;
      color: $orange_color;
      vertical-align: middle;
      margin-right: $colum;
    }
    a {
      font-size: $font-size;
      color: #23527c;
    }
  }
  .failure {
    .iconfont {
      font-size: $font-size*6;
      color: $gray_color;
      vertical-align: middle;
      margin-right: $colum;
    }
  }
  .wait {
    .iconfont {
      font-size: $font-size*6;
      color: red;
      vertical-align: middle;
      margin-right: $colum;
    }
  }
}

.validation_success, .validation_failure, .validation_waiting {
  .title {
    h2 {
      font-size: $font-size*2-4;
    }
  }
  .content {
    text-align: center;

    p {
      font-size: $font-size +8;
      color: $gray_color;

      .iconfont {
        font-size: $font-size*5;
        vertical-align: middle;
        margin-right: $colum;
        color: $orange_color;
        font-weight: bold;
      }
    }
    a {
      color: #337ab7
    }
  }
}

.validation_failure {
  .content {
    p {
      .iconfont {
        color: $gray_color;
      }
    }
  }
}

.validation_waiting {
  .content {
    p {
      .iconfont {
        color: red;
      }
    }
  }
}

